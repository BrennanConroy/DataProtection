{
  "version": "1.1.2",
  "description": "ASP.NET Core utilities for key derivation.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/dataprotection"
    },
    "tags": [
      "aspnetcore",
      "dataprotection"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Cryptography.Internal": "1.1.2",
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {}
  },
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  }
}